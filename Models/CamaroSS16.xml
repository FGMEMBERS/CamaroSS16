   <y-m> 0.03928   </y-m>
   <z-m> 0.41095 </z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>-1.0</y>
   <z>0.0</z>
  </axis>
 </animation>
<animation>
  <object-name>Steering</object-name>
  <type>rotate</type>
  <property>controls/flight/aileron</property>
<factor>80</factor>
  <offset-deg>0</offset-deg>
  <center>
   <x-m> -0.59102 </x-m>
   <y-m> -0.40635 </y-m>
   <z-m> 0.9645 </z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>
<animation>
 <type>material</type>
 <object-name>CamSS-RS3-Brakes</object-name>
<emission>
  <red>4.0</red>
  <green>0.0</green>
  <blue>0.0</blue>
  <factor-prop>/controls/gear/brake-parking</factor-prop>
 </emission>
</animation>
<animation>
<type>pick</type>
<object-name>D2</object-name>
<visible>true</visible>
<action>
<button>0</button>
<repeatable>false</repeatable>
<binding>
<command>nasal</command>
<script>aucafly.door.toggle();</script>
</binding>
</action>
<hovered>
            <binding>
                <command>set-tooltip</command>
                <tooltip-id>door</tooltip-id>
                <label>Door Open/Close</label>
            </binding>
        </hovered>

</animation>

